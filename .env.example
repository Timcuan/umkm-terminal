# Delay between deployments in milliseconds (default: 1000)
DEPLOY_DELAY=1000

# Rate limit per wallet in requests per second (0.5-10, default: 2)
# Prevents RPC throttling
RATE_LIMIT_PER_WALLET=2

# Maximum retries for failed deployments (default: 3)
MAX_RETRIES=3

# Delay between retries in milliseconds (default: 1000)
RETRY_DELAY=1000

# Gas price multiplier (1.0-2.0, default: 1.1)
# Higher values increase chance of inclusion but cost more
GAS_MULTIPLIER=1.1

# Maximum gas price in gwei (optional)
# Prevents deploying when gas is too high
MAX_GAS_PRICE=20

# Dry run mode - validate without deploying (true/false)
# Set to true to test configuration without spending gas
DRY_RUN=false

